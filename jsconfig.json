{
  "compilerOptions": {
    "baseUrl": "./src/",
    "module": "commonjs",
    "paths": {
      "components/*": ["components/*"],
      "constants/*": ["constants/*"],
      "fonts/*": ["fonts/*"],
      "images/*": ["images/*"],
      "services/*": ["services/*"],
      "store/*": ["store/*"],
      "styles/*": ["styles/*"],
      "util/*": ["util/*"]
    },
    "target": "es6"
  },
  "exclude": [
    "coverage",
    "docs",
    "node_modules"
  ]
}
